{
    "presets": [
        "@babel/env",
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
       "@babel/plugin-transform-runtime",
        "@babel/proposal-object-rest-spread",
      ["styled-jsx/babel", { "optimizeForSpeed": true }],
        ["babel-plugin-root-import", {
     "rootPathSuffix": "pages/",
     "rootPathPrefix": "~",

   }]

    ]
}
